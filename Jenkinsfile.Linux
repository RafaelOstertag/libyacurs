@Library('yapetJenkins') _

properties([buildDiscarder(logRotator(artifactDaysToKeepStr: '',
				      artifactNumToKeepStr: '',
				      daysToKeepStr: '', numToKeepStr:
				      '10'))])

node("linux") {
    github.notifyPendin()
    yapet.notify {
	os = "Linux"
	yapet.checkout()
        yapet.makeChangeLog()
	yapet.autoconf()
	yapet.configure os
	yapet.build os
    }
}
